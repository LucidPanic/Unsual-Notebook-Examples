    <span class="k">match</span> <span class="n">op</span> <span class="k">with</span>
    <span class="o">|</span> <span class="nc">Plus</span> <span class="o">-&gt;</span> <span class="o">(</span> <span class="o">+</span> <span class="o">)</span>
