    <span class="o">|</span> <span class="nc">Modulo</span> <span class="o">-&gt;</span> <span class="s2">&quot;%&quot;</span>
    <span class="o">|</span> <span class="nc">Expo</span> <span class="o">-&gt;</span> <span class="s2">&quot;^&quot;</span>
